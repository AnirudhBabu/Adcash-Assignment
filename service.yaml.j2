apiVersion: v1
kind: Service
metadata:
  name: gandalf-service
  annotations:
    service.beta.kubernetes.io/azure-load-balancer-resource-group: {{ resource_group }}
spec:
  type: LoadBalancer
  loadBalancerIP: {{ static_ip }}
  ports:
  - port: 80
    targetPort: 80
    protocol: TCP
  selector:
    app: gandalf
